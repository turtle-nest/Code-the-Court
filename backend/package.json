{
  "name": "code-the-court",
  "version": "1.0.0",
  "description": "Portfolio project",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon server.js",
    "start": "node server.js",
    "build": "echo \"No backend build step\"",
    "test": "jest --detectOpenHandles",
    "test:api": "newman run tests/postman/Code-the-Court.postman_collection.json",
    "db:init": "psql -U postgres -d code_the_court -f migrations/001_init_db.sql",
    "db:create": "createdb -U postgres code_the_court",
    "db:seed": "psql -U postgres -d code_the_court -f migrations/002_seed_data.sql",
    "db:reset": "./scripts/reset_db.sh && npm run db:seed",
    "check:judilibre": "node scripts/checkJudilibreId.mjs",
    "scan:judilibre": "node scripts/checkJudilibreId.mjs --scan"
  },
  "repository": {
    "type": "git",
